    "import re\n",
    "\n",
    "name = 'my roll number is 13'\n",
    "\n",
    "digitcount = re.sub(\"[^0-9]\",\"\",name)\n",
    "digitletter = re.sub(\"[^a-zA-Z]\",\"\",name)\n",
    "digitspace = re.findall(\"[ \\n]\",name) \n",
    "                    \n",
    "print(len(digitcount))\n",
    "print(len(digitletter))\n",
    "print(len(digitspace))"
   ]
